// main.js - put your code here
 $(document).ready(function() {
   // do stuff when DOM is ready
	 
	 $("a").click(function() {
	     alert("Hello world!");
	   });
	 

	 
	 $.getJSON("http://crunch.kmi.open.ac.uk/people/~dsherlock/services/%20JSONexample.r",
			    function(data){
			      $('body').html(data.name).css("color","green");  
			    });
	 
 });